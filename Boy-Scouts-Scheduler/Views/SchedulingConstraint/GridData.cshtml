@model IEnumerable<Boy_Scouts_Scheduler.Models.SchedulingConstraint>

@foreach (var item in Model) {
    <tr data-pkey="@item.ID">
        <td>
            @if (item.Group == null && item.GroupType == null) {
                @: All
            }
            @Html.DisplayFor(modelItem => item.Group.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GroupType.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Station.Name)
        </td>
        <td align="center">
            @Html.DisplayFor(modelItem => item.VisitNum)
        </td>
        <td class="rowControl hidden">
            <a href="#makeRowEditable" class="makeRowEditable">Edit</a> |
            <a href="#deleteRow" class="deleteRow">Delete</a>
        </td>
    </tr>
}
